    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npm run dev:app\" \"npm run dev:worker1\" \"npm run dev:worker2\"",
    "dev:app": "nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm' src/index.ts",
    "dev:worker1": "nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm' src/workers/manualFileProcessingWorker.ts",
    "dev:worker2": "nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm' src/workers/manualFileDeletionWorker.ts",
